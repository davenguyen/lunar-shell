<% satellite_partial = "lunar_shell/satellites/#{command}/_output" %>

<% if lookup_context.exists?(satellite_partial) %>
  <%= render satellite_partial.gsub('/_', '/') %>
<% else %>
  $('#shell-history').append('<%= j render('output') %>');
  $('#shell-cli').html('<%= j render('/lunar_shell/shells/cli') %>');
<% end %>
